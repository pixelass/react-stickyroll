{
	"check-coverage": true,
	"all": true,
	"extension": [
		".js",
		".jsx",
		".ts",
		".tsx"
	],
	"include": [
		"src/**/*.ts?(x)"
	],
	"exclude": [
		"**/*.d.ts",
		"src/hash-code.ts",
		"src/scroll-to.ts",
		"src/throttle.ts"
	],
	"reporter": [
		"html",
		"lcov",
		"text",
		"text-summary"
	],
	"report-dir": "coverage"
}
